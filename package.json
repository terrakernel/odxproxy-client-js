{
  "name": "@terrakernel/odxproxy-client-js",
  "version": "0.1.2",
  "description": "Official JavaScript/TypeScript client for ODXProxy",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.mjs.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "odoo",
    "odoo-integration",
    "odxproxy",
    "api",
    "sdk",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrakernel/odxproxy-client-js.git"
  },
  "bugs": {
    "url": "https://github.com/terrakernel/odxproxy-client-js/issues"
  },
  "homepage": "https://github.com/terrakernel/odxproxy-client-js#readme",
  "author": "Julian Richie Wajong <julian.wajong@gmail.com> (https://www.odxproxy.io)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv": "^17.2.2",
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.4.1",
    "tsup": "^8.0.1",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ulid": "^2.3.0"
  }
}
